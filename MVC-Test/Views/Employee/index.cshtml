@{
    //Layout = null;
    ViewBag.Title = "Employee Page";
}
@using Microsoft.AspNetCore.Authorization;
@using Microsoft.AspNetCore.Mvc;
@using Group6Application.Models;
@model Group6Application.Models.EmployeeView

<html>
<div class="text-center">
    <h1 class="display-4">Employee</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Middle Initial</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            <th>Supervisor ID</th>
            <th>Department ID</th>
            <th>Start Date</th>
            <th>Wage</th>
            <th>Email</th>
            <th>Address</th>
            <th>Title</th>
        </tr>
        <tr>

            @{

                foreach (EmployeeTemplate dept in Model.Employee)
                {
                <tr>
                    <th>@dept.ID</th>
                    <th>@dept.FirstName</th>
                    <th>@dept.Middle_Initial</th>
                    <th>@dept.LastName</th>
                    <th>@dept.Phone_Number</th>
                    <th>@dept.Supervisor_ID</th>
                    <th>@dept.Department_ID</th>
                    <th>@dept.Start_Date</th>
                    <th>@dept.Wage</th>
                    <th>@dept.Email</th>
                    <th>@dept.Address</th>
                    <th>@dept.Title</th>
                </tr>
                }

            }
    </table>

</div>

</html>